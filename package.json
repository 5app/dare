{
  "name": "dare",
  "version": "0.30.1",
  "description": "Database to REST, REST to Database",
  "main": "src/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run spec && (nyc report --reporter=text-lcov | coveralls)",
    "test:ci": "eslint ./ && nyc mocha test/specs/**/*.js --reporter mocha-circleci-reporter && (nyc report --reporter=text-lcov | coveralls)",
    "spec": "nyc mocha test/specs/**/*.js",
    "lint": "eslint ./",
    "lint-fix": "eslint --fix ./",
    "lint-diff": "LIST=`git diff-index --name-only HEAD | grep '.*\\.js$';`; if [ \"$LIST\" ]; then eslint $LIST; fi",
    "precommit-msg": "echo 'Running pre-commit checks... (skip using --no-verify)' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint-diff",
    "spec"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/5app/dare.git"
  },
  "keywords": [
    "Database",
    "MySQL",
    "REST"
  ],
  "author": "Andrew Dodson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/5app/dare/issues"
  },
  "homepage": "https://github.com/5app/dare#readme",
  "files": [
    "src/"
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.6.0",
    "eslint-config-5app": "^0.3.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^6.0.0",
    "mocha-circleci-reporter": "0.0.3",
    "nyc": "^13.0.1",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {}
}
